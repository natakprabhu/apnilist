{
  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "https://alyidbbieegylgvdqmis.supabase.co/storage/v1/object/public/sitemaps/sitemap.xml",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/articles/:slug",
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": ".*(googlebot|bingbot|yandex|baiduspider|twitterbot|facebookexternalhit|rogerbot|linkedinbot|embedly|quora link preview|showyoubot|outbrain|pinterest|slackbot|vkShare|W3C_Validator|whatsapp).*"
        }
      ],
      "destination": "/static_html_cache/:slug.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}