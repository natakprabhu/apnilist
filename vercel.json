{
  "cleanUrls": true,

  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "https://alyidbbieegylgvdqmis.supabase.co/storage/v1/object/public/sitemaps/sitemap.xml",
      "permanent": true
    }
  ],

  "rewrites": [
    {
      "source": "/articles/:slug",
      "destination": "/static_html_cache/:slug.html"
    },
    {
      "source": "/static_html_cache/:path*",
      "destination": "/static_html_cache/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
