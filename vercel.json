{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/articles/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "https://alyidbbieegylgvdqmis.supabase.co/storage/v1/object/public/sitemaps/sitemap.xml",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/articles/:slug",
      "destination": "/articles/:slug.html"
    }
  ]
}
