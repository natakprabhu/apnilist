{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "https://alyidbbieegylgvdqmis.supabase.co/storage/v1/object/public/sitemaps/sitemap.xml",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/articles/:slug",
      "destination": "/articles/:slug.html",
      "continue": true
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}